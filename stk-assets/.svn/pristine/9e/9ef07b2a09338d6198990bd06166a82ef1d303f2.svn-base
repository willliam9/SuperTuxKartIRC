void onWallCollision(int idKart, const string libraryInstance, const string objID)
{
    //Utils::logInfo("Wall Collision! Kart " + idKart + " with obj " + objID + " from " + libraryInstance);
    blowUpWall(libraryInstance);
}

void blowUpWall(string instID)
{
    Track::TrackObject@ wall = Track::getTrackObject(instID, "Wall");
    wall.setEnabled(false);
    
    Track::TrackObject@ part = Track::getTrackObject(instID, "Cube.000");
    part.setEnabled(true);
    
    Track::TrackObject@ part2 = Track::getTrackObject(instID, "Cube.001");
    part2.setEnabled(true);
    
    Track::TrackObject@ part3 = Track::getTrackObject(instID, "Cube.002");
    part3.setEnabled(true);
    
    Track::TrackObject@ part4 = Track::getTrackObject(instID, "Cube.003");
    part4.setEnabled(true);
    
    Track::TrackObject@ part5 = Track::getTrackObject(instID, "Cube.004");
    part5.setEnabled(true);
    
    Track::TrackObject@ part6 = Track::getTrackObject(instID, "Cube.005");
    part6.setEnabled(true);
    
    Track::TrackObject@ part7 = Track::getTrackObject(instID, "Cube.006");
    part7.setEnabled(true);
    
    Track::TrackObject@ part8 = Track::getTrackObject(instID, "Cube.007");
    part8.setEnabled(true);
    
    Track::TrackObject@ part9 = Track::getTrackObject(instID, "Cube.008");
    part9.setEnabled(true);
    
    Track::TrackObject@ part10 = Track::getTrackObject(instID, "Cube.009");
    part10.setEnabled(true);
    
    Track::TrackObject@ part11 = Track::getTrackObject(instID, "Cube.010");
    part11.setEnabled(true);
    
    Track::TrackObject@ part12 = Track::getTrackObject(instID, "Cube.011");
    part12.setEnabled(true);
}
